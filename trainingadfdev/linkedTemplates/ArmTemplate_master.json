{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "trainingadfdev"
		},
		"AzureSqlDatabase_Source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase_Source'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=trainingsource.database.windows.net;Initial Catalog=trainingsource;User ID=trainingsource_admin"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://trainingdatalakesource.dfs.core.windows.net"
		},
		"AzureDataLakeStorage1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "881ac007-74a7-4784-bce0-9aefb00920fc"
		},
		"AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "98946144-763b-4aaa-ac3c-4f3626379faa"
		},
		"AzureDataLakeStorage_sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://trainingdatalakedev.dfs.core.windows.net"
		},
		"AzureDataLakeStorage_sink_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "881ac007-74a7-4784-bce0-9aefb00920fc"
		},
		"AzureDataLakeStorage_sink_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "98946144-763b-4aaa-ac3c-4f3626379faa"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://trainingkeyvaultdev.vault.azure.net/"
		},
		"RestService_source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://airlabs.co/api/v9/airlines"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "trainingadfdev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase_Source_connectionString": {
						"value": "[parameters('AzureSqlDatabase_Source_connectionString')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"RestService_source_properties_typeProperties_url": {
						"value": "[parameters('RestService_source_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureDataLakeStorage_sink_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_sink_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage_sink_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStorage_sink_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStorage_sink_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStorage_sink_properties_typeProperties_servicePrincipalId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}